#include<stdio.h>
 
float add(float x, float y);
 
int main()
{
    float num1, num2, a;
    printf("\nAdd First Number:");
    scanf("%f", &num1);
    printf("\nAdd Second Number:");
    scanf("%f", &num2);
    a = add(num1, num2);
    printf("\nAddition of Two Numbers: %f", a);
    printf("\n");
    return 0;
}
 
float add(float x, float y)
{
    float sum;
    sum = x + y;
    return sum;
}
